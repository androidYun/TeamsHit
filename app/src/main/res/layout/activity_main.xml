<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/main_top_navi"
        layout="@layout/include_main_navi"
        android:layout_alignParentTop="true"></include>

    <FrameLayout
        android:id="@+id/framelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/mian_navi"
        android:layout_below="@+id/main_top_navi"
        android:background="@color/white"></FrameLayout>

    <include
        android:id="@+id/mian_navi"
        layout="@layout/include_navi_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"></include>
</RelativeLayout>